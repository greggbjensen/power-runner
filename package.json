{
  "name": "powerrunner",
  "productName": "PowerRunner",
  "version": "1.2.1",
  "license": "MIT",
  "main": "electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && SET NODE_ENV=dev && electron ./dist/electron/main.js",
    "build": "tsc -p tsconfig.electron.json && ng build --base-href ./ && copyfiles ./src/electron/preload.js ./dist/powerrunner --up 2",
    "build-prod": "tsc -p tsconfig.electron.json && ng build --prod --base-href ./ && copyfiles ./src/electron/preload.js ./dist/powerrunner --up 2",
    "copy-deps": "copyfiles ./package.json ./dist && copyfiles ./src/electron/powershell/*.ps1 ./dist --up 1 && copyfiles ./node_modules/**/*.* ./dist && copyfiles ./src/electron/preload.js ./dist/powerrunner --up 2",
    "package": "npm run copy-deps && npm run build-prod && electron-packager ./dist --overwrite --out=./release",
    "installer": "node installer.js",
    "package-installer": "npm run package && npm run installer",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild --version 9.0.3",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.10",
    "@angular/cdk": "^13.3.8",
    "@angular/common": "~13.3.10",
    "@angular/compiler": "~13.3.10",
    "@angular/core": "~13.3.10",
    "@angular/forms": "~13.3.10",
    "@angular/material": "^13.3.8",
    "@angular/material-moment-adapter": "^13.3.8",
    "@angular/platform-browser": "~13.3.10",
    "@angular/platform-browser-dynamic": "~13.3.10",
    "@angular/router": "~13.3.10",
    "@types/underscore": "^1.11.4",
    "@w11k/ngx-componentdestroyed": "^5.0.2",
    "angular-split": "^13.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-unhandled": "^4.0.1",
    "esm": "^3.2.25",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.1",
    "is-elevated": "^4.0.0",
    "js-yaml": "^4.1.0",
    "ngx-autosize-input": "^13.1.0",
    "ngx-markdown": "^13.1.0",
    "node-abi": "^3.22.0",
    "node-fetch": "^3.2.4",
    "node-pty": "^0.10.1",
    "node-yaml": "^4.0.1",
    "rxjs": "~7.5.5",
    "ts-md5": "^1.2.11",
    "tslib": "^2.4.0",
    "underscore": "^1.13.3",
    "xregexp": "^5.1.0",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-search": "^0.8.2",
    "xterm-addon-web-links": "^0.5.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.10",
    "@angular/language-service": "~13.3.10",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.11.7",
    "@types/sqlite3": "^3.1.8",
    "codelyzer": "^6.0.2",
    "copyfiles": "^2.4.1",
    "electron": "^19.0.1",
    "electron-packager": "^15.5.1",
    "electron-rebuild": "^3.2.7",
    "electron-reload": "2.0.0-alpha.1",
    "electron-winstaller": "^5.0.0",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.8.0",
    "tslint": "~6.1.3",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "4.6.4"
  },
  "build": {
    "extraResources": [
      "./electron/powershell/**"
    ]
  }
}