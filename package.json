{
  "name": "powerrunner",
  "productName": "PowerRunner",
  "version": "0.0.7",
  "license": "MIT",
  "main": "electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "yarn build && SET NODE_ENV=dev && electron ./dist/electron/main.js",
    "build": "tsc -p tsconfig.electron.json && ng build --base-href ./",
    "build-prod": "tsc -p tsconfig.electron.json && ng build --prod --base-href ./",
    "copy-deps": "copyfiles ./package.json ./dist && copyfiles ./node_modules/**/*.* ./dist",
    "package": "yarn copy-deps && yarn build-prod && electron-packager ./dist --overwrite --out=./release",
    "installer": "node installer.js",
    "package-installer": "yarn package && yarn installer",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.6",
    "@angular/cdk": "^8.2.0",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@types/underscore": "^1.9.2",
    "@w11k/ngx-componentdestroyed": "^4.1.4",
    "angular-split": "^3.0.2",
    "electron-is-dev": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "hammerjs": "^2.0.8",
    "js-yaml": "^3.13.1",
    "node-pty": "^0.9.0-beta19",
    "node-yaml": "^4.0.1",
    "rxjs": "~6.4.0",
    "ts-md5": "^1.2.7",
    "tslib": "^1.10.0",
    "underscore": "^1.9.1",
    "xregexp": "^4.2.4",
    "xterm": "^4.3.0",
    "xterm-addon-fit": "^0.3.0",
    "xterm-addon-search": "^0.4.0",
    "xterm-addon-web-links": "^0.2.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.7",
    "codelyzer": "^5.0.0",
    "copyfiles": "^2.1.1",
    "electron": "^6.1.6",
    "electron-packager": "^14.1.1",
    "electron-rebuild": "^1.8.8",
    "electron-reload": "^1.5.0",
    "electron-winstaller": "^4.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "typescript": "3.5.3"
  }
}
